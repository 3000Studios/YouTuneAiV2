{
  "name": "youtuneai-theme",
  "version": "1.0.0",
  "description": "YouTuneAI WordPress FSE Theme with 3D/VR capabilities",
  "author": "3000Studios",
  "license": "Commercial",
  "scripts": {
    "build:tailwind": "tailwindcss -i ./assets/css/src/main.css -o ./assets/css/dist/main.css",
